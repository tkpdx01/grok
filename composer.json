{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-bcmath": "*",
        "abovesky/dcat-lightbox": "^1.0",
        "abovesky/dcat-media-player": "^1.0",
        "bitwasp/bitcoin": "^1.0",
        "canbez/dcat-theme": "^1.0",
        "cin/dcat-config": "^1.0",
        "connmix/connmix": "^1.0",
        "dcat/easy-excel": "^1.0",
        "dcat/laravel-admin": "2.*",
        "dcat/laravel-log-viewer": "^0.1.7",
        "death_satan/dcat-wang-editor": "^1.0",
        "earnp/laravel-google-authenticator": "dev-master",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3.0",
        "intervention/image": "^2.7",
        "jacobcyl/ali-oss-storage": "2.1",
        "kkokk/poster": "^1.2",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "mosiboom/dcat-iframe-tab": "^1.2",
        "overtrue/easy-sms": "^1.3",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "1.3.*",
        "spatie/laravel-short-schedule": "^1.4",
        "super-eggs/dcat-distpicker": "2.0",
        "tymon/jwt-auth": "^1.0",
        "web3p/ethereum-util": "^0.1.3",
        "workerman/gateway-worker": "^3.0",
        "workerman/gatewayclient": "3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Units/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
